<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>ArtG</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <div class="main-container">

  <nav>
    <h1><a href="/">ArtG</a></h1>
    <ul>
      <% const user = null %>

      <% if (user) { %>
        <li>Welcome, <%= user.name %></li>
        <li><a href="/api/user/logout">Log out</a></li>
      <% } else { %>
        <li><a href="/api/user/login">Log in</a></li>
        <li><a href="/api/user/register" class="btn">Sign up</a></li>
      <% } %>
    </ul>
  </nav>
  </div>